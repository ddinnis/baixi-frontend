<!--components/custom-navbar/custom-navbar.wxml-->
<view class="nav-bar">
	<view class="status" style="height:{{statusHeight}}px;"></view>
	<view class="nav">
		<view class="left">
			<view class="slot">
				<slot name="left"></slot>
			</view>
			<view class="default">
				<van-icon class="icon" name="arrow-left" bindtap="onArrowLeftTap"/>
			</view>
		</view>
		<view class="center">
			<view class="slot">
				<slot name="center"></slot>
			</view>
			<view class="default">{{title}}</view>
			<view class="right"></view>
		</view>
	</view>
</view>