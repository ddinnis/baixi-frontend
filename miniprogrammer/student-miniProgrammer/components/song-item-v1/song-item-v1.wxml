<!--components/song-item-v1/song-item-v1.wxml-->
<view class="song-item" bindtap="onSongItemTap">
	<image class="img" src="{{itemData.al.picUrl}}"></image>
	<view class="info">
		<view class="name">{{itemData.name}}</view>
		<view class="singer">{{itemData.ar[0].name}}</view>
	</view>
	<view class="arrow">
		<van-icon name="arrow" />
	</view>
</view>